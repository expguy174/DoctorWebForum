
@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="container-fluid">
    <div class="card shadow mb-4">
        <div class="card-header py-3 d-flex justify-content-between align-items-center">
            <h6 class="m-0 font-weight-bold text-primary">All admin</h6>
            <div>
                <a href="#" class="d-flex justify-content-center align-items-center"><span> Create new</span><span class="fa fa-plus p-2"> </span></a>
            </div>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th class="text-center">ID</th>
                            <th class="text-center">USERNAME</th>
                            <th class="text-center">PASSWORD</th>
                            <th class="text-center">EMAIL</th>
                            <th class="text-center">EDIT</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="text-center">Tiger Nixon</td>
                            <td class="text-center">System Architect</td>
                            <td class="text-center">Edinburgh</td>
                            <td class="text-center">$320,800</td>
                            <td class="d-flex justify-content-around align-content-center">
                                <a href="#" class="fa fa-edit"></a>
                                <a href="#" class="fa fa-trash"></a>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

